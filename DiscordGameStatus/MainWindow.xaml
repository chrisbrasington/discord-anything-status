<Window x:Class="DiscordGameStatus.LaunchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiscordGameStatus"
        mc:Ignorable="d"
        Title="Discord Game Status" Height="150" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <!--<RowDefinition/>-->
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <!--<Label Grid.Row="0" Name="Description" Content="Content" Margin="5"/>-->
        <ComboBox Grid.Row="0" Name="GameList"></ComboBox>
        <TextBox Grid.Row="1" Name="Input" Margin="5"/>
        <Button Grid.Row="2" Content="GO" Margin="5" Click="Button_Click"/>
    </Grid>
</Window>
